-- samples loop around. 808 has 6, so 0-5 are unique, and 6=0, 7=1, ...
d1 $ n "0*3 1*3 6*4" # s "808"
d1 $ n "0..6" # s "808"

-- parseBP_E to manually convert strings into pattern
d1 $ n (parseBP_E $ unwords $ map show [0..5]) # s "808"

